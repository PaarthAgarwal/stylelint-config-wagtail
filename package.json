{
  "name": "stylelint-config-wagtail",
  "version": "0.0.1",
  "description": "Stylelint config rules for Wagtail styles (Saas)",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./ --max-warnings 0",
    "prettier:check": "prettier --check '**/**.{js,scss}'",
    "prettier": "prettier --write '**/**.{js,scss}'",
    "preflight": "npm run lint && npm run prettier && npm run test"
  },
  "dependencies": {
    "stylelint-scss": "^3.6.0"
  },
  "devDependencies": {
    "eslint": "^2.12.0",
    "eslint-config-wagtail": "0.1.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "jest": "24.7.1",
    "prettier": "^1.17.0",
    "stylelint": "^10.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wagtail/stylelint-config-wagtail.git"
  },
  "keywords": [
    "stylelint",
    "stylelint-config",
    "wagtail"
  ],
  "author": "wagtail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wagtail/stylelint-config-wagtail/issues"
  },
  "homepage": "https://github.com/wagtail/stylelint-config-wagtail#readme",
  "prettier": {
    "singleQuote": true
  }
}
